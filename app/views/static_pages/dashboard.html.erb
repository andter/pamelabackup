<% if current_user.present? %>
<h4><%=current_user.email.capitalize%>'s Dashboard </h4>
<%= link_to "My Pictures", user_photos_path(current_user), class: "dashbutton"%>
<%= link_to "My Messages", messages_path, class: "dashbutton"%>
<%= link_to "Edit Profile", edit_user_path(current_user), class: "dashbutton"%>
<% else %>
<h3>You are not logged in! </h3>
<h4>Please Login or Signup before proceeding</h4>
<%= link_to "Login", new_user_session_path %> | 
<%= link_to "Signup", new_user_path%>
<% end %>

