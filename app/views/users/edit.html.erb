<h3>Edit User </h3>
<%= form_for @user do |f| %>
<p>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :phonenumber %>
<%= f.text_field :phonenumber %>
<%= f.label :password %>
<%= f.password_field :password %>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
</br>
<%= f.submit "Update"%>
<% end %>
